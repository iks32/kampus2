import javax.swing.*;

class Latihan1Javax {
    
    public static void main(String[] args) { // agar dapat dijalankan sebagai class utama
        JFrame myWindow;
        
        myWindow = new JFrame(); // pembuatan objek JFrame
        myWindow.setSize(300, 200); // mengatur ukuran form
        myWindow.setTitle("My First Java Program"); // mengatur judul untuk form
        myWindow.setVisible(true); // untuk menampilkan form
    }
}
